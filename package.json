{
	"name": "vasvscrapper",
	"version": "0.0.2-alpha.1",
	"license": "MIT",
	"description": "the-vasv-deets backend",
	"author": "P. Soumith Reddy",
	"repository": {
		"type": "git",
		"url": "https://github.com/soumith2105/the-vasv-deets/vasvscrapper"
	},
	"devDependencies": {
		"husky": "^6.0.0",
		"lint-staged": "^11.0.0"
	},
	"lint-staged": {
		"**/*.py": [
			"yarn lint"
		]
	},
	"scripts": {
		"prepare": "husky install && if [ ! -d \".venv\" ]; then virtualenv .venv; fi && poetry install",
		"lint": "poetry run black . && poetry run flake8",
		"shell": "poetry shell",
		"test": "poetry run pytest",
		"release": "./release.sh"
	}
}
